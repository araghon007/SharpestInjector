<Window x:Class="SharpestInjectorGUI.CallExport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SharpestInjectorGUI"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="Call an exported function" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <ListBox x:Name="ProcessModules" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" SelectionChanged="ProcessModules_SelectionChanged" Margin="15"/>
        <ListBox x:Name="ModuleExports" Grid.Row="1" Grid.Column="0" SelectionChanged="ModuleExports_SelectionChanged" Margin="15"/>

    </Grid>
</Window>
